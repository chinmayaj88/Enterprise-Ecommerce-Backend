{
  "server": {
    "port": 3004,
    "env": "development",
    "requestTimeout": 30000,
    "shutdownTimeout": 30000,
    "maxRequestSize": "10mb"
  },
  "database": {
    "url": "postgresql://user:password@localhost:5432/cart_db"
  },
  "services": {
    "product": "http://localhost:3003"
  },
  "cart": {
    "expirationDays": 30,
    "maxItems": 100,
    "maxItemQuantity": 99
  },
  "jwt": {
    "secret": "your-secret-key-change-in-production"
  },
  "redis": {
    "url": "redis://localhost:6379"
  },
  "rateLimit": {
    "windowMs": 900000,
    "maxRequests": 100
  },
  "cors": {
    "allowedOrigins": []
  },
  "logging": {
    "level": "info"
  }
}

